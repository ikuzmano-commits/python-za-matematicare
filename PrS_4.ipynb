   "metadata": {},
   "outputs": [],
   "source": [
    "rjesenja = sp.solve(x**3 - 2*x+1, x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8ab52bc9",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"Rješenja jednadžbe x^2 - 2 = 0 su:\", rjesenja)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "288ceb88",
   "metadata": {},
   "outputs": [],
   "source": [
    "x, y = sp.symbols('x y')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
