   "metadata": {},
   "outputs": [],
   "source": [
    "rj = sp.solve([x + y - 3, x - y - 1], [x, y])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ac867001",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"Rješenje sustava:\", rj)"
   ]
  }
