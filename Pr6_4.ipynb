   "source": [
    "import sympy as sp\n",
    "x = sp.symbols('x')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5216efe5",
   "metadata": {},
   "outputs": [],
   "source": [
    "rjesenja = sp.solve(x**3 - 2*x+1, x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "79033b2d",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"Rješenja jednadžbe x^3 - 2x+1 = 0 su:\", rjesenja)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fc411fc0",
   "metadata": {},
   "outputs": [],
   "source": [
    "x, y = sp.symbols('x y')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4d0b682b",
   "metadata": {},
   "outputs": [],
   "source": [
    "rj = sp.solve([x + y - 3, x - y - 1], [x, y])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d9491920",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"Rješenje sustava:\", rj)"
   ]
  }
