   "source": [
    "import sympy as sp\n",
    "x = sp.symbols('x')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "73e7dc4f",
   "metadata": {
    "lines_to_next_cell": 2
   },
   "outputs": [],
   "source": [
    "f = (x**3 - 1)/(x - 1)\n",
    "print(\"f(x):\", f)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "24bf6d84",
   "metadata": {},
   "outputs": [],
   "source": [
    "fs = sp.simplify(f)\n",
    "print(\"Pojednostavljeni izraz f(x):\", fs)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d29a409b",
   "metadata": {},
   "outputs": [],
   "source": [
    "# računanje limesa u točki x -> 1\n",
    "l = sp.limit(f, x, 1)\n",
    "print(\"Limes f(x) kada x -> 1:\", l)"
   ]
  }
