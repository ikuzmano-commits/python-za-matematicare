   "source": [
    "import sympy as sp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "29af8aa8",
   "metadata": {},
   "outputs": [],
   "source": [
    "x = sp.symbols('x')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "dbb63f2d",
   "metadata": {},
   "outputs": [],
   "source": [
    "f = x**2 * sp.exp(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fa482bd7",
   "metadata": {},
   "outputs": [],
   "source": [
    "F = sp.integrate(f, x)\n",
    "I = sp.integrate(f, (x, 0, 1))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b0df5a41",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"Neodređeni integral:\", F)\n",
    "print(\"Određeni integral od 0 do 1:\", I)"
   ]
  }
 ],
