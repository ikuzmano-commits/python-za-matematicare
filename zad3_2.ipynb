   "source": [
    "def savrsen(n):\n",
    "    zbroj=0;\n",
    "    for i in range(1,n):\n",
    "        if n%i==0:\n",
    "            zbroj=zbroj+i\n",
    "    if n==zbroj:\n",
    "        print(f'Broj {n} je savrsen.')   \n",
    "    else:\n",
    "        print(f'Broj {n} nije savrsen.') "
   ]
  },
