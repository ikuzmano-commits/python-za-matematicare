    "def Coll(n):\n",
    "    korak=0\n",
    "    while(n!=1):\n",
    "        if n%2==0:\n",
    "            n=n//2\n",
    "        else:\n",
    "            n=n*3+1\n",
    "        print(n)\n",
    "        korak=korak+1\n",
    "    return korak"
   ]
