   "source": [
    "def rekfib(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return rekfib(n - 1) + rekfib(n - 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
