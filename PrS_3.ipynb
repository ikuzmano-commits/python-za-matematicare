   "source": [
    "import sympy as sp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "78ea0ae3",
   "metadata": {},
   "outputs": [],
   "source": [
    "x = sp.symbols('x')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "bde2b8a6",
   "metadata": {},
   "outputs": [],
   "source": [
    "f = x**2 * sp.exp(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fe1584f1",
   "metadata": {},
   "outputs": [],
   "source": [
    "F = sp.integrate(f, x)\n",
    "I = sp.integrate(f, (x, 0, 1))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a28b2e1a",
   "metadata": {
    "lines_to_next_cell": 2
   },
   "outputs": [],
   "source": [
    "print(\"Neodređeni integral:\", F)\n",
    "print(\"Određeni integral od 0 do 1:\", I)"
   ]
  }
 ],
